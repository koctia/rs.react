{
  "name": "components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node ./server.tsx",
    "server": "nodemon",
    "dev": "eslint --ext .ts,.tsx src/ && vite",
    "build": "eslint src --ext js,ts,tsx && prettier --check . && tsc --noEmit && vite build",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .ts,.tsx src/",
    "coverage": "vitest run --coverage",
    "cypress": "cypress open",
    "coverage:e2e": "cypress run --e2e && nyc report --reporter=text-summary",
    "coverage:screen": "npx rimraf coverage && npx cypress run --e2e && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-hooks": "^1.0.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "cypress": "^12.10.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.58.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.29.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  }
}
